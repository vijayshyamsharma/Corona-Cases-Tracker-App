<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jai.covid_19trackerapp">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/coivdlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coivdlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".States"
            android:screenOrientation="sensorPortrait"></activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AllDetaills" android:screenOrientation="sensorPortrait">

        </activity>
    </application>

</manifest>